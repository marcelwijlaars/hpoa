<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/120814-042457/Help/stylesheets/help-topic.xsl"?>
<topic topic-id="DDevicePowerSequenceInterconnectBaystab" xmlns:fn="http://www.w3.org/2005/02/xpath-functions">
  <title>Device Power Sequence Interconnect Bays tab</title>
  <source>/120814-042457/Help/en/topics/DDevicePowerSequenceInterconnectBaystab.xml</source>
  <keywords>interconnect bays power sequence power sequence interconnect bays</keywords>
  <intro />
  <sections>
    <section>
      <desc />
      <content>
        <p class="tablespacerbefore" />
        <table width="100%" style="border-collapse:collapse;" cellspacing="0" cellpadding="2">
          <tr>
            <th class="tableheading" style="border:1px solid #010101;text-align:left;">Column
</th>
            <th class="tableheading" style="border:1px solid #010101;text-align:left;">Description
</th>
          </tr>
          <tr valign="top">
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Bay
</p>
            </td>
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Bay number of the device
</p>
            </td>
          </tr>
          <tr valign="top">
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Device
</p>
            </td>
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">The type of device in the bay or Absent if no device is installed in the bay
</p>
            </td>
          </tr>
          <tr valign="top">
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Enabled
</p>
            </td>
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Enables power sequencing, disables power sequencing, or does not allow powering on of the device if No Poweron is selected.
</p>
            </td>
          </tr>
          <tr valign="top">
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">Delay
</p>
            </td>
            <td style="border:1px solid #010101;" align="left">
              <p class="tablebodytext">The amount of delay, in seconds, before the device powers on. Possible delay values are 1 to 3600.
</p>
            </td>
          </tr>
        </table>
        <p class="bodytext">Click <b>Apply</b> to save settings.</p>
      </content>
    </section>
  </sections>
</topic>