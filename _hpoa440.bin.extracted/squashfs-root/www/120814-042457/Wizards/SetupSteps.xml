<Wizard>
  <wizardTitle>firstTimeSetupWizard</wizardTitle>
  <wizardSubTitle>setupInitialSettings</wizardSubTitle>
  <wizardAccessLevel>ADMINISTRATOR</wizardAccessLevel>
  <wizardOaAccess>true</wizardOaAccess>
  <wizardMinimumLoad>false</wizardMinimumLoad>
  <linkNavigation>true</linkNavigation>
  <loadScript></loadScript>
  <unloadScript></unloadScript>
  <steps>
    <step>
      <number>0</number>
      <title>welcome</title>
      <content>/120814-042457/Wizards/SetupContent_Welcome.xsl</content>
      <helpContext>WIZ_WELCOME</helpContext>
      <initFunction>initWelcome</initFunction>
    </step>
	<step>
      <number>1</number>
      <title>fips</title>
      <content>/120814-042457/Templates/fips.xsl</content>
      <helpContext>WIZ_FIPS_MODE</helpContext>
      <initFunction>initFipsPage</initFunction>
    </step>
    <step>
      <number>2</number>
      <title>enclosureSelection</title>
      <content>/120814-042457/Wizards/SetupContent_EncSelect.xsl</content>
      <initFunction>initEncSelect</initFunction>
      <helpContext>WIZ_ENC_SELECTION</helpContext>
      <allowFirmwareMismatch>false</allowFirmwareMismatch>
      <allowPrimaryExclusion>false</allowPrimaryExclusion>
      <eventListeners>
        <listener type="topologyComplete" body="initEncSelect('/120814-042457/Wizards/SetupContent_EncSelect.xsl');reconcilePage();"/>
      </eventListeners>
    </step>
    <step>
      <number>3</number>
      <title>configManagement</title>
      <content>/120814-042457/Wizards/SetupContent_ManageConfig.xsl</content>
      <helpContext>WIZ_ENC_UPLOAD_SCRIPT</helpContext>
      <initFunction>initConfigManagement</initFunction>
    </step>
    <step>
      <number>4</number>
      <title>rackAndEnclosureSettings</title>
      <content>/120814-042457/Wizards/SetupContent_EncSettings.xsl</content>
      <initFunction>initEncSettings</initFunction>
      <helpContext>WIZ_ENC_SETTINGS</helpContext>
    </step>
    <step>
      <number>5</number>
      <title>adminAccountSetup</title>
      <content>/120814-042457/Wizards/SetupContent_AdminPassword.xsl</content>
      <helpContext>WIZ_ENC_ADMIN_SETUP</helpContext>
      <initFunction>initAdminAccountSetup</initFunction>
    </step>
    <step>
      <number>6</number>
      <title>localUserAccounts</title>
      <content>/120814-042457/Wizards/SetupContent_UserOverview.xsl</content>
      <helpContext>WIZ_USER_OVERVIEW</helpContext>
      <initFunction>initUserOverviewPage</initFunction>
      <eventListeners>
        <listener body="usersChangedHandler(result)">
          <params>
            <param>result</param>
          </params>
          <events>
            <event>EVENT_USER_ADDED</event>
            <event>EVENT_USER_DELETED</event>
            <event>EVENT_USER_INFO_CHANGED</event>
            <event>EVENT_ADMIN_RIGHTS_CHANGED</event>
            <event>EVENT_USER_PERMISSION</event>
          </events>
        </listener>
      </eventListeners>
      <steps>
        <step>
          <number>6.1</number>
          <title>userSettings</title>
          <content>/120814-042457/Wizards/SetupContent_Users.xsl</content>
          <helpContext>WIZ_USER_EDIT</helpContext>
          <initFunction>initUserSettings</initFunction>
        </step>
      </steps>
    </step>
    <step>
      <number>7</number>
      <title>ebipa</title>
      <content>/120814-042457/Wizards/SetupContent_EBIPA.xsl</content>
      <initFunction>initEBIPAIntro</initFunction>
      <helpContext>WIZ_EBIPA_WELCOME</helpContext>
      <steps>
        <step>
          <number>7.1</number>
          <title>ebipaSettings</title>
          <content>SetupContent_EBIPASettings.xsl</content>
          <initFunction>initEBIPASettings</initFunction>
          <helpContext>WIZ_EBIPAv4_FINISH</helpContext>
          <buttons>
            <button id="cancelButton" stringKey="cancel" enabled="true" method="getWizardTop().cancelSubWizard('wizardContainerPage.html?step=8')"/>
            <button id="skipButton" stringKey="skip" disabled="false" style="margin-left:40px;" method="location='wizardContainerPage.html?step=7.2'"/>
            <button id="nextButton" stringKey="next" disabled="false" focus="true" method="saveEbipaAllSettings()"/>
            <button id="previousButton" stringKey="previous" disabled="false" method="location='wizardContainerPage.html?step=7'"/>
          </buttons>
        </step>
        <step>
          <number>7.2</number>
          <title>ebipav6Settings</title>
          <content>SetupContent_EBIPAv6Settings.xsl</content>
          <initFunction>initEBIPAv6Settings</initFunction>
          <helpContext>WIZ_EBIPAv6_FINISH</helpContext>
          <buttons>
            <button id="cancelButton" stringKey="cancel" enabled="true" method="getWizardTop().cancelSubWizard('wizardContainerPage.html?step=8')"/>
            <button id="skipButton" stringKey="skip" disabled="false" style="margin-left:40px;" method="location='wizardContainerPage.html?step=8'"/>
            <button id="nextButton" stringKey="next" disabled="false" focus="true" method="saveEbipav6AllSettings()"/>
            <button id="previousButton" stringKey="previous" disabled="false" method="location='wizardContainerPage.html?step=7.1'"/>
          </buttons>
        </step>
      </steps>
    </step>
	  <step>
		  <number>8</number>
		  <title>directoryGroups</title>
		  <content>/120814-042457/Wizards/SetupContent_LDAPGroups.xsl</content>
		  <helpContext>WIZ_LDAP_GROUPS</helpContext>
		  <initFunction>initDirectoryGroups</initFunction>
		  <eventListeners>
			  <listener body="groupChangedHandler(result)">
				  <params>
					  <param>result</param>
				  </params>
				  <events>
					  <event>EVENT_LDAPGROUP_ADDED</event>
					  <event>EVENT_LDAPGROUP_DELETED</event>
					  <event>EVENT_LDAPGROUP_ADMIN_RIGHTS_CHANGED</event>
					  <event>EVENT_LDAPGROUP_INFO_CHANGED</event>
					  <event>EVENT_LDAPGROUP_PERMISSION</event>
				  </events>
			  </listener>
		  </eventListeners>
		  <steps>
			  <step>
				  <number>8.1</number>
				  <title>groupSettings</title>
				  <content>/120814-042457/Wizards/SetupContent_LDAPGroupSettings.xsl</content>
				  <initFunction>initDirectoryGroupSettings</initFunction>
			  </step>
		  </steps>
	  </step>
    <step>
      <number>9</number>
      <title>directorySettings</title>
      <content>/120814-042457/Wizards/SetupContent_LDAP.xsl</content>
      <helpContext>WIZ_LDAP_CONFIG</helpContext>
      <initFunction>initDirectorySettings</initFunction>
    </step>
    <step>
      <number>10</number>
      <title>oaNetworkSettings</title>
      <content>/120814-042457/Wizards/SetupContent_EMNetSettingsFront.xsl</content>
      <initFunction>initEBIPAIntro</initFunction>
      <helpContext>WIZ_OA_NETWORK_WELCOME</helpContext>
      <steps>
        <step>
          <number>10.1</number>          
          <title>forIPv4</title>
          <content>/120814-042457/Wizards/SetupContent_EMNetSettings.xsl</content>
          <helpContext>WIZ_OA_NETWORK_SETTINGS</helpContext>
          <initFunction>initOaNetworkSettingsIpv4</initFunction>
          <buttons>
            <button id="cancelButton" stringKey="cancel" enabled="true" method="getWizardTop().cancelSubWizard('wizardContainerPage.html?step=11')"/>
            <button id="skipButton" stringKey="skip" disabled="false" style="margin-left:40px;" method="location='wizardContainerPage.html?step=10.2'"/>
            <button id="nextButton" stringKey="next" disabled="false" focus="true" method="setupOaNetSettings()"/>
            <button id="previousButton" stringKey="previous" disabled="false" method="location='wizardContainerPage.html?step=10'"/>
          </buttons>
        </step>
        <step>
          <number>10.2</number>          
          <title>forIPv6</title>
          <content>/120814-042457/Wizards/SetupContent_EMNetSettings.xsl</content>
          <helpContext>WIZ_OA_NETWORK_SETTINGSv6</helpContext>
          <initFunction>initOaNetworkSettingsIpv6</initFunction>
          <buttons>
            <button id="cancelButton" stringKey="cancel" enabled="true" method="getWizardTop().cancelSubWizard('wizardContainerPage.html?step=11')"/>
            <button id="skipButton" stringKey="skip" disabled="false" style="margin-left:40px;" method="location='wizardContainerPage.html?step=11'"/>
            <button id="nextButton" stringKey="next" disabled="false" focus="true" method="setupOaNetSettingsIpv6()"/>
            <button id="previousButton" stringKey="previous" disabled="false" method="location='wizardContainerPage.html?step=10.1'"/>
          </buttons>
        </step>
      </steps>
    </step>
    <step>
      <number>11</number>
      <title>snmpSettings</title>
      <content>/120814-042457/Wizards/SetupContent_SNMP.xsl</content>
      <helpContext>WIZ_ENC_SNMP_SETTINGS</helpContext>
      <initFunction>initSnmp</initFunction>
      <eventListeners>
        <listener body="snmpInfoChangedHandler(result)">
          <params>
            <param>result</param>
          </params>
          <events>
            <event>EVENT_SNMP_INFO_CHANGED</event>
            <event>EVENT_SNMPV3_INFO_CHANGED</event>
          </events>
        </listener>
      </eventListeners>
      <steps>
        <step singleton="true">
          <number>11.1</number>          
          <title>addSnmpUser</title>
          <content>/120814-042457/Wizards/SetupContent_SnmpNewUser.xsl</content>
          <helpContext>WIZ_ENC_SNMP_ADD_USER</helpContext>
          <initFunction>initNewSnmpUserSettings</initFunction>
        </step>
        <step singleton="true">
          <number>11.2</number>          
          <title>addSnmpTrap</title>
          <content>/120814-042457/Wizards/SetupContent_SnmpNewTrap.xsl</content>
          <helpContext>WIZ_ENC_SNMP_ADD_ALERT</helpContext>
          <initFunction>initNewSnmpTrapSettings</initFunction>
        </step>
      </steps>
    </step>
    <step>
      <number>12</number>
      <title>powerManagement</title>
      <content>/120814-042457/Wizards/SetupContent_Power.xsl</content>
      <helpContext>WIZ_POWER_MGMT</helpContext>
      <initFunction>initPowerManagement</initFunction>
    </step>
    <step>
      <number>13</number>
      <title>finish</title>
      <content>/120814-042457/Wizards/SetupContent_Finish.xsl</content>
      <helpContext>WIZ_FINISH</helpContext>
      <initFunction>initFinish</initFunction>
    </step>
  </steps>
</Wizard>
